!!!
%html
  %head
    %title Tst
    = stylesheet_link_tag    'application', media: 'all'
    = javascript_include_tag 'application'
    = csrf_meta_tags
  %body
    - if user_signed_in?
      =link_to('Logout', destroy_user_session_path, :method => :delete)
    - else
      = link_to('Login', new_user_session_path)
    = yield
